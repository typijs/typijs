{
  "name": "cms-server",
  "version": "0.0.1",
  "license": "MIT",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "preserve": "tsc -p ../cms-api",
    "serve": "node dist/server.js",
    "predev": "tsc -p ../cms-api && tsc",
    "dev": "concurrently \"tsc -w -p ../cms-api\" \"set NODE_ENV=development && nodemon --inspect --watch ../cms-api/dist dist/server.js\"",
    "prod": "concurrently \"tsc -p server && set NODE_ENV=production && node dist/server.js\""
  },
  "author": "danghung1202@gmail.com",
  "dependencies": {
    "@angular-cms/api": "0.0.1",
    "express": "^4.17.1",
    "tslib": "^1.11.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.1",
    "@types/node": "^12.7.8",
    "concurrently": "^3.5.1",
    "nodemon": "^1.19.2",
    "ts-node": "^8.4.1",
    "typescript": "~3.5.3"
  }
}